@model IEnumerable<WebApiClient.Models.Item>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<script src="~/Scripts/jquery-3.4.1.js"></script>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table" id="tblItem">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.qtyStock)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        <th></th>
    </tr>
</table>

<script>
    $(document).ready(function () {
       // alert("Hello");
        $.getJSON("https://localhost:44368/api/Items/", function (data) {
            $.each(data, function (key, value) {
              //  alert(value.name);
                var row = "<tr><td>"  + value.name  + "</td><td>" + value.qtyStock + "</td><td>" + value.description + "</td></tr>";
                $("#tblItem").append(row);
            });
        });
    })
</script>
    